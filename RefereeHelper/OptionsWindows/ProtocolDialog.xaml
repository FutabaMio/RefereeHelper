<Window x:Class="RefereeHelper.OptionsWindows.ProtocolDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RefereeHelper.OptionsWindows"
        mc:Ignorable="d"
        Title="Протоколы" Height="85" Width="410">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="44*"/>
            <RowDefinition Height="44"/>

        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width ="200"/>
            <ColumnDefinition Width="200"/>
        </Grid.ColumnDefinitions>
        <Grid Grid.Row="1" Grid.Column="0">
            <Button Name="AbsolBut" Content="Абсолютный протокол" Grid.Row="0" FontSize="12" Margin="10,10,10,10" Click="AbsolBut_Click"/>
        </Grid>
        <Grid Grid.Row="1" Grid.Column="1">
            <Button Name="GroupBut" Content="Протокол по катигориям" Grid.Row="0" FontSize="12" Margin="10,10,10,10" Click="GroupBut_Click"/>
        </Grid>
    </Grid>
</Window>
